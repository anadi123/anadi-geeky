#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n;
	cin>>n;
	if(n==2||n==3)
	{
		cout<<"No Solution";
	}
	else if(n==1)
	{
		cout<<"1";
	}
	else
	{
		if(n%2==0)
		{
			for(int i=2;i<=n;i=i+2)
		
			{
				cout<<i<<" ";
			}	
			for(int i=1;i<=n;i=i+2)
			{
				cout<<i<<" ";
			}
		}
		else
		{
			for(int i=n-1;i>=1;i-=2)
			{
				cout<<i<<" ";
			}
			for(int i=n;i>=1;i-=2)
			{
				cout<<i<<" ";
			}
		}
		/*for(int i=1;i<=n;i=i+2)
		{
			a[k++]=i;
		}	
		for(int i=2;i<=n;i=i+2)
		{
			a[k++]=i;
		}
		for(int i=1;i<k;i++)
		{
			if(abs(a[i]-a[i-1])==1)
			{
				cout<<n<<'\n';
				break;
			}
			else
			cout<<"Correct";
			*/
		
	}	
	
	return 0;
}
